    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="~/Content/plugins/favicon.ico" />
    <!--SCRIPTS-->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")

