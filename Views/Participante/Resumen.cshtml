        function cont() {
            $(".cont").each(function () {
                var cont = $("#" + this.id);
                var total = parseInt(cont.html().split("/")[1]);
                cont.html("0/" + total);
            });

            $('input[type=checkbox]').each(function () {
                if (this.checked) {
                    var id = this.id.split("-")[1];
                    var cont = $("#" + id);
                    var cant = parseInt(cont.html().split("/")[0]);
                    var total = parseInt(cont.html().split("/")[1]);
                    cont.html((cant + 1) + "/" + total);
                }
            });
        }

