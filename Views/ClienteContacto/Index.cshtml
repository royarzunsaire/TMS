@using SGC.Helpers
@model SGC.Models.viewModelRepresentanteEmpresa

@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    ViewBag.Title = "Index";
}
<h3 class="page-title">
    Administracion Categorias R11
</h3>
<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-folder"></i>Cotizaciones del representante empresa
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse"> </a>
                    <a href="javascript:;" class="fullscreen"> </a>
                </div>
            </div>
            <div class="portlet-body">

                <table class="table table-striped table-bordered table-hover dt-responsive dataTable" id="sys-table" cellspacing="0">

                    <thead>
                        <tr>
                            <th>
                                Nombre cotizacion
                            </th>
                            <th>
                                Vigencia
                            </th>
                            <th>
                                Opciones
                            </th>
                        </tr>

                    </thead>

                    <tbody>
                        @foreach (var item in Model.cotizaciones)
                        {
                            <tr>

                                <td>

                                        <div class="input-group-btn">

                                        </div>
                                    
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section topLevel{
    @{Html.RenderPartial("_RenderConfirm.Modal"); }
}
